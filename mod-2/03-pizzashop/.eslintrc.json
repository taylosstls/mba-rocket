{
  "extends": [
    "@rocketseat/eslint-config/react",
    "eslint:recommended",
    "plugin:prettier/recommended",
    "prettier"
  ],
  "plugins": ["simple-import-sort"],
  "rules": {
    "simple-import-sort/imports": "error",
    "simple-import-sort/exports": "warn"
  },
  "parserOptions": {
    "sourceType": "module",
    "ecmaVersion": "latest"
  }
}
